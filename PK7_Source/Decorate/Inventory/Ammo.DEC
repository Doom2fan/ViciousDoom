actor VD_9mmCartridges : ammo replaces Clip {
    tag "$9MMCARTRIDGES"
    inventory.PickupMessage "$PICKUP_9MM"
    inventory.Amount 15
    inventory.MaxAmount 300
    ammo.BackpackMaxAmount 600
    inventory.Icon "CLIPA0"
    inventory.PickupSound "misc/ammopickup"
